<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.skteam.diyodardayari">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_diyodar"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_diyodar"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppNewPro">
        <activity android:name="com.skteam.diyodardayari.activity.PrivacyPolicyActivity" />
        <activity android:name="com.skteam.diyodardayari.activity.HomeActivity" />
        <activity android:name="com.skteam.diyodardayari.activity.OtpActivity" />
        <activity android:name="com.skteam.diyodardayari.activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.skteam.diyodardayari.MainActivity" />



        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="@string/admob_app_id"/>


    </application>

</manifest>